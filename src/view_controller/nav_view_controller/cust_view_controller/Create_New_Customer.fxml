<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="530.0" prefWidth="640.0"
            xmlns="http://javafx.com/javafx/15.0.1"
            fx:controller="view_controller.nav_view_controller.cust_view_controller.Create_New_Customer">
   <VBox layoutX="193.0" layoutY="105.0" prefHeight="321.0" prefWidth="254.0">
      <HBox prefHeight="37.0" prefWidth="254.0">
         <Label text="Name">
            <HBox.margin>
               <Insets top="5.0"/>
            </HBox.margin>
         </Label>
         <TextField fx:id="custNameTF">
            <HBox.margin>
               <Insets left="60.0"/>
            </HBox.margin>
         </TextField>
      </HBox>
      <HBox layoutX="10.0" layoutY="124.0" prefHeight="37.0" prefWidth="226.0">
         <VBox.margin>
            <Insets top="20.0"/>
         </VBox.margin>
         <Label text="Phone">
            <HBox.margin>
               <Insets top="5.0"/>
            </HBox.margin>
         </Label>
         <TextField fx:id="custPhoneTF">
            <HBox.margin>
               <Insets left="59.0"/>
            </HBox.margin>
         </TextField>
      </HBox>
      <HBox layoutX="10.0" layoutY="10.0" prefHeight="37.0" prefWidth="226.0">
         <VBox.margin>
            <Insets top="20.0"/>
         </VBox.margin>
         <Label text="Street">
            <HBox.margin>
               <Insets top="5.0"/>
            </HBox.margin>
         </Label>
         <TextField fx:id="custAddressTF">
            <HBox.margin>
               <Insets left="60.0"/>
            </HBox.margin>
         </TextField>
      </HBox>
      <HBox layoutX="10.0" layoutY="47.0" prefHeight="37.0" prefWidth="226.0">
         <VBox.margin>
            <Insets top="20.0"/>
         </VBox.margin>
         <Label text="Postal Code">
            <HBox.margin>
               <Insets top="5.0"/>
            </HBox.margin>
         </Label>
         <TextField fx:id="custPostalTF">
            <HBox.margin>
               <Insets left="28.0"/>
            </HBox.margin>
         </TextField>
      </HBox>
      <HBox layoutX="10.0" layoutY="84.0" prefHeight="37.0" prefWidth="226.0">
         <VBox.margin>
            <Insets top="20.0"/>
         </VBox.margin>
         <Label text="Country">
            <HBox.margin>
               <Insets top="5.0"/>
            </HBox.margin>
         </Label>
         <ComboBox fx:id="custCountryCB" onAction="#generateDivisionComboBox" prefWidth="150.0">
            <HBox.margin>
               <Insets left="60.0"/>
            </HBox.margin>
         </ComboBox>
      </HBox>
      <HBox layoutX="10.0" layoutY="84.0" prefHeight="37.0" prefWidth="226.0">
         <VBox.margin>
            <Insets top="20.0"/>
         </VBox.margin>
         <Label text="State/Province">
            <HBox.margin>
               <Insets top="5.0"/>
            </HBox.margin>
         </Label>
         <ComboBox fx:id="custFldCB" prefWidth="150.0">
            <HBox.margin>
               <Insets left="25.0"/>
            </HBox.margin>
         </ComboBox>
      </HBox>
      <Button fx:id="custSubmitButton" mnemonicParsing="false" onAction="#saveNewCustomer" text="Submit">
         <VBox.margin>
            <Insets left="100.0" top="20.0"/>
         </VBox.margin>
      </Button>
   </VBox>
</AnchorPane>
